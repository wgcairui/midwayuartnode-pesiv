    "@midwayjs/cli": "^1.2.76",
    "@midwayjs/core": "^2.11.4",
    "@midwayjs/decorator": "^2.11.1",
    "@types/crc": "^3.4.0",
    "cross-env": "^7.0.3",
    "types-uart": "^1.1.2"
  },
  "dependencies": {
    "@cairui/midway-io.client": "^1.0.3",
    "@cairui/midway-tcpserver": "^1.0.2",
    "@midwayjs/bootstrap": "^2.11.4",
    "axios": "^0.21.1",
    "crc": "^3.8.0"
  }
}
